{
	"name": "@adonisjs/ally",
	"version": "3.2.1",
	"description": "Social authentication provider for AdonisJS",
	"types": "./build/adonis-typings/index.d.ts",
	"main": "build/providers/AllyProvider.js",
	"files": [
		"build/adonis-typings",
		"build/providers",
		"build/templates",
		"build/src",
		"build/instructions.js",
		"build/instructions.md",
		"build/standalone.d.ts",
		"build/standalone.js"
	],
	"scripts": {
		"mrm": "mrm --preset=@adonisjs/mrm-preset",
		"pretest": "npm run lint",
		"test": "node japaFile.js",
		"clean": "del build",
		"start": "node -r @adonisjs/assembler/build/register examples/app.ts",
		"copyfiles": "copyfiles \"templates/**/*.txt\" \"instructions.md\" build",
		"compile": "npm run lint && npm run clean && tsc",
		"build": "npm run compile && npm run copyfiles",
		"prepublishOnly": "npm run build",
		"lint": "eslint . --ext=.ts",
		"format": "prettier --write .",
		"commit": "git-cz",
		"release": "np",
		"version": "npm run build",
		"sync-labels": "github-label-sync --labels ./node_modules/@adonisjs/mrm-preset/gh-labels.json adonisjs/adonis-ally"
	},
	"keywords": [
		"adonis",
		"adonisjs",
		"social-auth",
		"oauth"
	],
	"author": "adonisjs,virk",
	"license": "MIT",
	"devDependencies": {
		"@adonisjs/assembler": "^5.3.0",
		"@adonisjs/core": "^5.1.7",
		"@adonisjs/mrm-preset": "^4.0.1",
		"@adonisjs/require-ts": "^2.0.4",
		"@poppinss/dev-utils": "^1.1.3",
		"@types/node": "^15.6.0",
		"commitizen": "^4.2.4",
		"copyfiles": "^2.4.1",
		"cz-conventional-changelog": "^3.3.0",
		"del-cli": "^3.0.1",
		"dotenv": "^10.0.0",
		"eslint": "^7.27.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-adonis": "^1.3.1",
		"eslint-plugin-prettier": "^3.4.0",
		"github-label-sync": "^2.0.0",
		"husky": "^6.0.0",
		"japa": "^3.1.1",
		"mrm": "^3.0.2",
		"nock": "^13.0.11",
		"np": "^7.5.0",
		"prettier": "^2.3.0",
		"typescript": "^4.2.4"
	},
	"husky": {
		"hooks": {
			"commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
		}
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"np": {
		"contents": ".",
		"anyBranch": false
	},
	"dependencies": {
		"@poppinss/oauth-client": "^2.0.3",
		"@poppinss/utils": "^3.1.2"
	},
	"peerDependencies": {
		"@adonisjs/core": "^5.1.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/adonisjs/adonis-ally.git"
	},
	"bugs": {
		"url": "https://github.com/adonisjs/adonis-ally/issues"
	},
	"homepage": "https://github.com/adonisjs/adonis-ally#readme",
	"publishConfig": {
		"tag": "latest",
		"access": "public"
	},
	"adonisjs": {
		"instructions": "./build/instructions.js",
		"instructionsMd": "./build/instructions.md",
		"types": "@adonisjs/ally",
		"providers": [
			"@adonisjs/ally"
		]
	}
}
